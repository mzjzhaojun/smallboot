<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhengqing.system.mapper.SysAppConfigMapper">

    <select id="selectDataList" resultType="com.zhengqing.system.model.bo.SysAppConfigBO">
        select *
        from t_sys_app_config
        <where>
            <if test="filter.idList != null and filter.idList.size() > 0">
                AND id IN
                <foreach item="id" index="index" collection="filter.idList" open="(" separator="," close=")">
                    #{id}
                </foreach>
            </if>
            <if test="filter.appIdList != null and filter.appIdList.size() > 0">
                AND app_id IN
                <foreach item="appId" index="index" collection="filter.appIdList" open="(" separator="," close=")">
                    #{appId}
                </foreach>
            </if>
        </where>

    </select>
</mapper>
